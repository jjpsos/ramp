#, php-format
msgid "<strong>%1s #%2s</strong> in the series</p>"
msgstr "<strong>%1s #%2s</strong> in der Serie</p>"

#: sliced-invoices-recurring.php:390
#, php-format
msgctxt "%s = human-readable time difference"
msgid "<p>Next %1s will be published on:<br><strong>%2s (in %3s)</strong></p>"
msgstr ""
"<p>Die nächsten %1s werden veröffentlicht am:<br><strong>%2s (in %3s)</"
"strong></p>"

#: sliced-invoices-recurring.php:392
#, php-format
msgid "<p>Recurring frequency is every: <strong>%1s </strong></p>"
msgstr ""
"<p>Die sich wiederholende Häufigkeit ist alle: <strong>%1s</strong></p>"

#: sliced-invoices-recurring.php:394
#, php-format
msgid "<p>Automatically send to client: <strong>%1s </strong></p>"
msgstr "<p>Automatisch an Kunden senden: <strong>%1s</strong></p>"

